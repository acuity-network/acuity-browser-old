{
  "name": "mix-acuity",
  "productName": "MIX Acuity",
  "version": "1.2.4",
  "author": "Jonathan Brown <jbrown@mix-blockchain.org>",
  "description": "Reference application for MIX Blockchain.",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron": "ts-node download.ts && electron-builder install-app-deps",
    "electron:dev": "electron-webpack dev",
    "electron:build": "electron-webpack && electron-builder"
  },
  "main": "background.js",
  "electronWebpack": {
    "staticSourceDirectory": "public",
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "build": {
    "asarUnpack": [
      "**/node_modules/sharp/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "pkg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "7z"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "tar.xz"
      ],
      "category": "Network"
    }
  },
  "dependencies": {
    "@acuitysocial/api": "^0.0.0",
    "@mdi/font": "^2.8.94",
    "@parity/api": "^5.1.5",
    "@pinata/sdk": "^1.1.3",
    "@polkadot/api": "^1.16.1",
    "async-mutex": "^0.1.4",
    "axios": "^0.19.2",
    "bip32": "^2.0.3",
    "bip39": "^3.0.2",
    "brotli": "^1.3.2",
    "bs58": "^4.0.1",
    "buefy": "^0.8.2",
    "core-js": "^3.4.5",
    "detect-browser": "^5.0.0",
    "download": "^7.1.0",
    "electron-context-menu": "^0.15.0",
    "electron-window-state": "^5.0.3",
    "ethereumjs-common": "^1.5.0",
    "ethereumjs-tx": "^2.1.2",
    "events": "^3.0.0",
    "google-protobuf": "^3.5.0",
    "ipfs": "^0.44.0",
    "it-to-stream": "^0.1.1",
    "just-throttle": "^1.1.0",
    "level": "^6.0.0",
    "lexicographic-integer-encoding": "^1.0.1",
    "multihashes": "^0.4.13",
    "pica": "^5.1.0",
    "qrcode": "^1.2.0",
    "register-service-worker": "^1.6.2",
    "setimmediate": "^1.0.5",
    "sharp": "^0.24.0",
    "source-map-support": "^0.5.12",
    "subleveldown": "^5.0.0",
    "twemoji": "12.0.4",
    "unused-filename": "^2.1.0",
    "videostream": "^3.2.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.10.0",
    "vue-markdown-v2": "^0.1.7",
    "vue-router": "^3.1.3",
    "vue-simple-progress": "^1.1.0",
    "vue-timeago": "^5.0.0",
    "vuex": "^3.1.2",
    "web3": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/download": "^6.2.4",
    "@types/just-throttle": "^1.1.0",
    "@types/node": "12",
    "@types/qrcode": "^1.3.4",
    "@types/sharp": "^0.24.0",
    "@types/subleveldown": "^4.1.0",
    "@types/twemoji": "^12.1.0",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-router": "^4.4.1",
    "@vue/cli-plugin-typescript": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "css-loader": "^3.5.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^3.2.0",
    "electron-webpack-vue": "^2.3.0",
    "node-sass": "^4.12.0",
    "patch-package": "^6.1.4",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "vue-devtools": "^5.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2"
  },
  "resolutions": {
    "keccak": "3.0.0",
    "websocket": "1.0.31"
  }
}
